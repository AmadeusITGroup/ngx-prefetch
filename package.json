{
  "name": "@o3r/ngx-prefetch",
  "version": "14.0.2",
  "description": "Angular builder for prefetching resources before loading the application",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p ./tsconfig.json && copyfiles src/schema.json ./dist --up 1 && copyfiles src/templates/* ./dist --up 1",
    "test": "jest",
    "lint": "eslint ./src",
    "clean": "rimraf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AmadeusITGroup/ngx-prefetch.git"
  },
  "keywords": [
    "angular",
    "builder",
    "prefetch",
    "service worker"
  ],
  "files": [
    "dist/",
    "builders.json"
  ],
  "author": "Victor Scaiceanu <victor.scaiceanu@amadeus.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AmadeusITGroup/ngx-prefetch/issues"
  },
  "builders": "builders.json",
  "homepage": "https://github.com/AmadeusITGroup/ngx-prefetch#readme",
  "dependencies": {
    "@angular-devkit/architect": "^0.1402.3",
    "@angular-devkit/core": "^14.2.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "comment-json": "^4.1.0",
    "eslint": "^8.0.0",
    "mustache": "^4.2.0",
    "path": "^0.12.7",
    "typescript": "~4.8.0",
    "webpack": "^5.50.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/mustache": "^4.1.2",
    "@types/node": "^16.0.0",
    "copyfiles": "^2.4.1",
    "jest": "^29.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.0"
  },
  "resolutions": {
    "minimist": "1.2.6",
    "ansi-regex": "5.0.1",
    "tmpl": "1.0.5",
    "tar": "6.1.11"
  }
}
