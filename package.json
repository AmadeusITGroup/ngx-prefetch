{
  "name": "ngx-prefetch",
  "version": "12.0.0",
  "description": "Angular builder for prefetching resources before loading the application",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p ./tsconfig.json && cpx src/schema.json ./dist && cpx src/templates/* ./dist/templates",
    "test": "jest --config ./jest-ut.config.js",
    "lint": "eslint ./src",
    "clean": "rimraf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vscaiceanu-1a/ngx-prefetch.git"
  },
  "keywords": [
    "angular",
    "builder",
    "prefetch",
    "service worker"
  ],
  "author": "Victor Scaiceanu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vscaiceanu-1a/ngx-prefetch/issues"
  },
  "builders": "builders.json",
  "homepage": "https://github.com/vscaiceanu-1a/ngx-prefetch#readme",
  "dependencies": {
    "@angular-devkit/architect": "^0.1200.4",
    "@angular-devkit/core": "^12.0.4",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "comment-json": "^4.1.0",
    "eslint": "^7.28.0",
    "mustache": "^4.2.0",
    "path": "^0.12.7",
    "typescript": "^4.3.3",
    "webpack": "^5.50.0"
  },
  "devDependencies": {
    "@angular/service-worker": "^12.1.4",
    "@types/mustache": "^4.1.2",
    "@types/node": "^15.12.2",
    "cpx": "^1.5.0",
    "rimraf": "^3.0.2"
  }
}
