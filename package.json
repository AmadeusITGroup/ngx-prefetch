{
  "name": "@o3r/ngx-prefetch",
  "version": "0.0.0",
  "description": "Angular builder for prefetching resources before loading the application",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p ./tsconfig.json && copyfiles src/schema.json ./dist --up 1 && copyfiles src/templates/* ./dist --up 1 && copyfiles src/schematics/ng-add/schema.json ./dist --up 1",
    "test": "jest",
    "lint": "eslint ./src",
    "clean": "rimraf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AmadeusITGroup/ngx-prefetch.git"
  },
  "keywords": [
    "otter-module",
    "angular",
    "builder",
    "prefetch",
    "service worker"
  ],
  "files": [
    "dist/",
    "builders.json",
    "collection.json"
  ],
  "schematics": "./collection.json",
  "author": "Victor Scaiceanu <victor.scaiceanu@amadeus.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AmadeusITGroup/ngx-prefetch/issues"
  },
  "builders": "builders.json",
  "homepage": "https://github.com/AmadeusITGroup/ngx-prefetch#readme",
  "dependencies": {
    "@angular-devkit/architect": "~0.2101.4",
    "@angular-devkit/core": "~21.1.4",
    "mustache": "^4.2.0",
    "typescript": "~5.9.3",
    "webpack": "^5.97.1"
  },
  "devDependencies": {
    "@angular-devkit/schematics": "~21.1.4",
    "@eslint/js": "^10.0.0",
    "@types/jest": "^29.5.14",
    "@types/mustache": "^4.2.6",
    "@types/node": "^24.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^10.0.0",
    "globals": "^17.0.0",
    "jest": "^29.7.0",
    "knip": "^5.45.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "type-fest": "^5.1.0",
    "typescript-eslint": "^8.20.0"
  },
  "packageManager": "yarn@4.12.0+sha512.f45ab632439a67f8bc759bf32ead036a1f413287b9042726b7cc4818b7b49e14e9423ba49b18f9e06ea4941c1ad062385b1d8760a8d5091a1a31e5f6219afca8"
}
