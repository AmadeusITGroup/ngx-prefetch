{
  "name": "my-ngsw-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "npm run build:prod && npm run server",
    "start:prod:ci": "npm run build:prod && npm run server:ci",
    "server": "http-server -p 8080 -c-1 dist --cors",
    "server:ci": "nohup http-server -p 8080 -c-1 dist/browser &",
    "build": "ng build",
    "build:prod": "npm run clean && ng build --configuration production && npm run generate:prefetch",
    "generate:prefetch": "ng run my-ngsw-app:generate-prefetch",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:e2e": "npx playwright test --config testing/playwright.config.ts --browser=chromium",
    "clean": "rimraf ./dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.1.0",
    "@angular/common": "^18.1.0",
    "@angular/compiler": "^18.1.0",
    "@angular/core": "^18.1.0",
    "@angular/forms": "^18.1.0",
    "@angular/platform-browser": "^18.1.0",
    "@angular/platform-browser-dynamic": "^18.1.0",
    "@angular/router": "^18.1.0",
    "@angular/service-worker": "^18.1.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.3",
    "zone.js": "~0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.0",
    "@angular/cli": "^18.0.2",
    "@angular/compiler-cli": "^18.1.0",
    "@o3r/ngx-prefetch": "../..",
    "@playwright/test": "^1.37.1",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^20.10.6",
    "http-server": "^14.1.1",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "rimraf": "^5.0.5",
    "typescript": "~5.4.5"
  },
  "engines": {
    "npm": ">=6.14.11",
    "node": ">=18",
    "yarn": "please-use-npm"
  }
}
