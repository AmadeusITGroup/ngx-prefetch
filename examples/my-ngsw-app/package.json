{
  "name": "my-ngsw-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "npm run build:prod && npm run server",
    "start:prod:ci": "npm run build:prod && npm run server:ci",
    "server": "http-server -p 8080 -c-1 dist",
    "server:ci": "nohup http-server -p 8080 -c-1 dist &",
    "build": "ng build",
    "build:prod": "npm run clean && ng build --configuration production && npm run generate:prefetch && cpy 'src/prefetch.html' dist",
    "generate:prefetch": "ng run my-ngsw-app:generate-prefetch",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:e2e": "npx playwright test --config testing/playwright.config.ts --browser=chromium",
    "clean": "rimraf ./dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.0",
    "@angular/common": "~12.1.0",
    "@angular/compiler": "~12.1.0",
    "@angular/core": "~12.1.0",
    "@angular/forms": "~12.1.0",
    "@angular/platform-browser": "~12.1.0",
    "@angular/platform-browser-dynamic": "~12.1.0",
    "@angular/router": "~12.1.0",
    "@angular/service-worker": "~12.1.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.11",
    "@angular/cli": "~12.2.10",
    "@angular/compiler-cli": "~12.1.0",
    "@o3r/ngx-prefetch": "../..",
    "@playwright/test": "^1.17.2",
    "@types/jasmine": "~3.10.3",
    "@types/node": "^17.0.8",
    "cpy-cli": "^3.1.1",
    "http-server": "^14.1.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.11",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "rimraf": "^3.0.2",
    "typescript": "~4.3.2"
  },
  "engines": {
    "npm": ">=6.14.11",
    "node": ">=14.16.0",
    "yarn": "please-use-npm"
  }
}
